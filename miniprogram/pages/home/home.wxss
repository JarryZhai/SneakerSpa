/* miniprogram/pages/home/home.wxss */
.logo {
  background-image: url("https://s1.ax1x.com/2020/06/10/tokrod.jpg");
  background-size: cover;
  height: 115rpx;
  display: flex;
  justify-content: center;
  padding-top: 50rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.pageTwo .swiperClass {margin: 0;margin-bottom: 20rpx; height: 400rpx; background-image: url("https://s1.ax1x.com/2020/06/10/toZnk4.jpg");background-size: cover;}
.pageTwo .slide-image {width: 100%;height: 80%; margin-top: 30rpx; border-radius: 10rpx;position: relative;box-shadow: 0 0 7rpx rgba(0, 0, 0, .8)}
.pageTwo image.active {transform: none;transition: all 0.2s ease-in 0s;}
.pageTwo image.quiet {transform: scale(0.8333333);transition: all 0.2s ease-in 0s;}

.placeholder-buttom {
  width: 100%;
  height: 100rpx;
}